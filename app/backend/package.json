{
  "name": "eslint-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=local ts-node src/api/server",
    "prestart": "npx prisma generate && npx prisma migrate dev --name init && npx prisma db seed",
    "migrate": "npx prisma migrate dev --name init",
    "seed": "npx prisma db seed",
    "generate": "npx prisma generate",
    "build": "rimraf dist && tsc -p .",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:verbose": "npm run test:verbose -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test:watch:coverage": "npm run test:coverage -- --watch",
    "test:watch:verbose": "npm run test:verbose -- --watch",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:verbose": "npm run test:verbose -- --watch -c jest-unit-config.js",
    "test:e2e": "npm run test -- --watch -c jest-e2e-config.js",
    "test:e2e:coverage": "npm run test:e2e -- --coverage",
    "test:e2e:verbose": "npm run test:verbose -- --watch -c jest-e2e-config.js",
    "test:staged": "npm test -- --findRelatedTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.5.1",
    "prisma": "^3.12.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  }
}
